{
  "name": "@toma-ct/n8n-nodes-ai-numbers",
  "description": "N8N node for communication with AI Numbers API",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://developer.ainumber.com/?shell#introduction",
  "author": {
    "name": "Toma Popoov",
    "email":  "toma.popov@currencytransfer.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://git@github.com:Tmpv/n8n-nodes-ai-numbers.git"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier credentials --write",
    "lint": "eslint credentials package.json",
    "lintfix": "eslint credentials package.json --fix"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AINumbersApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/AINumbers/AINumbers.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.57.1",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "version": "1.1.3",
  "bugs": {
    "url": "https://https://github.com/Tmpv/n8n-nodes-ai-numbers/issues"
  }
}
